<table>
  <thead>
    <tr>
      <th>菜品</th>
      <th>数量</th>
      <th>时间</th>
    </tr>
  </thead>
  <tbody>
    <% @order_items.each do |order_item| %>
    <tr>
      <td><%= order_item.item.name %></td>
      <td><%= order_item.count %></td>
      <td><%= order.created_at %></td>
    </tr>
    <% end %>
  </tbody>
</table>
